{
	"firestore": {
		"database": "(default)",
		"location": "nam5",
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"functions": [
		{
			"source": "functions",
			"codebase": "default",
			"ignore": [
				"node_modules",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log",
				"*.local"
			],
			"predeploy": [
				"npm --prefix \"$RESOURCE_DIR\" run lint",
				"npm --prefix \"$RESOURCE_DIR\" run build"
			]
		}
	],
	"storage": {
		"rules": "storage.rules"
	},
	"emulators": {
		"auth": {
			"port": 9100
		},
		"functions": {
			"port": 5002
		},
		"firestore": {
			"port": 8085
		},
		"storage": {
			"port": 9200
		},
		"ui": {
			"enabled": true,
			"port": 4050
		},
		"singleProjectMode": true
	}
}